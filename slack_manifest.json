{
    "display_information": {
        "name": "share"
    },
    "features": {
        "bot_user": {
            "display_name": "Share URL",
            "always_online": true
        }
    },
    "oauth_config": {
        "scopes": {
            "user": [
                "reactions:read",
                "channels:history"
            ],
            "bot": [
                "chat:write",
                "incoming-webhook",
                "links.embed:write",
                "links:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://asia-northeast1-radiant-voyage-325608.cloudfunctions.net/share",
            "user_events": [
                "reaction_added"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}
